@page
<div class="container">
    <div class="row">
        <div class="col-6">
            Usuário: <input type="text" id="userInput" />
        </div>
        <div class="col-6">&nbsp;</div>
    </div>
    <div class="row">Votação: Qual o seu canal favorito no Youtube?&nbsp;</div>
    <div class="row">
        <div class="col-12">
            <!-- input type="radio" name="group1" id="r1" value="1" /><label for="r1"> button one</label>-->
            <img width="25px" height="25px" src="data:image/jpeg;base64," alt="Avatar" class="image">
            <input type="radio" name="myChannel" id="goytacazFc" value="Goytacaz Futebol Clube" />
            <label for="goytacaz">Goytacaz Futebol Clube</label>
            <br /><span id="goytacazBlock"></span><br />

            <img width="25px" height="25px" src="data:image/jpeg;base64," alt="Avatar" class="image">
            <input type="radio" name="myChannel" id="americanoFc" value="Americano Futebol Clube" />
            <label for="americano"> Americano Futebol Clube</label>
            <br /><span id="americanoBlock"></span><br />

            <img width="25px" height="25px" src="https://lh3.googleusercontent.com/proxy/3Z0GAHL4avHUjwX357mJVAbzEq5j7jUXBTXIp9QVmooTa3OQWehMMX29_G2Z19OcYbgnVlt4ZJoUILagwBDATd6_eN2EVKd2vZYADRLjTTQ" alt="Avatar" class="image">
            <input type="radio" name="myChannel" id="camposFc" value="Campos Atlético Associação" />
            <label for="campos">Campos Atlético Associação</label>
            <br /><span id="camposBlock"></span><br />
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <input type="button" id="sendButton" value="Vote Now" />
            <hr />
        </div>
    </div>
    <div class="row">
        <div class="col-12">
            <ul id="messagesList"></ul>
        </div>
    </div>
</div>
<script src="~/lib/jquery/dist/jquery.min.js"></script>
<script src="~/lib/microsoft/signalr/dist/browser/signalr.js"></script>
<script src="~/js/poll.js"></script>
<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">